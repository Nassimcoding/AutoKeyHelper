<Window x:Class="AutoKeyHelper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AutoKeyHelper"
        mc:Ignorable="d"
        Title="MainWindow" Height="386" Width="582">
    <Grid>

        <Button Content="Camp1"
                HorizontalAlignment="Left"
                VerticalAlignment="Top"
                Click="MoveMouseToCenter_Click" Margin="11,185,0,0" Height="48" Width="66"/>

        <Button Content="MarketTest" HorizontalAlignment="Left" Margin="83,286,0,0" VerticalAlignment="Top" Click="MoveMouseToMarketTest" Height="42" Width="68"/>
        <Button Content="CanvasTest" HorizontalAlignment="Left" Height="42" Margin="83,238,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Width="68" Click="CanvasAddTrangleTest">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform ScaleY="1"/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <Button Content="Metrol3" HorizontalAlignment="Left" Height="42" Margin="11,238,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.58,0.638" Click="Button_Click_2" Width="67"/>

        <ScrollViewer HorizontalScrollBarVisibility="Auto"
              VerticalScrollBarVisibility="Disabled"
              Height="200" Width="400" Margin="172,159,10,12">
            <Canvas x:Name="MainCanvas" Background="LightGray" Height="200" Width="800"/>
        </ScrollViewer>
        <Button x:Name="StartGetPoint" Content="vision" HorizontalAlignment="Left" Height="42" Margin="11,286,0,0" VerticalAlignment="Top" Width="66" Click="StartGetPoint_Click"/>
        <TextBox x:Name="WritingB" HorizontalAlignment="Left" Height="31" Margin="11,328,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="280"/>
        <Button Content="zombie1" HorizontalAlignment="Left" Height="42" Margin="11,138,0,0" VerticalAlignment="Top" Width="66" Click="Button_Click_1"/>
        <Button Content="Toy2" HorizontalAlignment="Left" Height="48" Margin="83,185,0,0" VerticalAlignment="Top" Width="68" Click="Button_Click_3"/>
        <Button Content="sDPrey" Margin="84,137,431,191" Click="Button_Click_4"/>
        <Button Content="DPrey" Margin="11,10,504,318" Click="Button_Click_5"/>
        <Button Content="LPrey" Margin="84,10,431,318"/>
        <Button Content="FPrey" Margin="156,10,359,318"/>
        <Label Content="Inputkeys" HorizontalAlignment="Left" Height="27" Margin="172,68,0,0" VerticalAlignment="Top" Width="79" RenderTransformOrigin="-1.68,2.236"/>
        <TextBox x:Name="InputKeys1" HorizontalAlignment="Left" Height="22" Margin="172,95,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="130"/>
        <TextBox x:Name="LoopTime1" HorizontalAlignment="Left" Height="22" Margin="335,96,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="130"/>
        <Label Content="LoopTime(Secind)" HorizontalAlignment="Left" Height="27" Margin="335,69,0,0" VerticalAlignment="Top" Width="130"/>
        <Label Content="Resolution" HorizontalAlignment="Left" Height="27" Margin="11,60,0,0" VerticalAlignment="Top" Width="79"/>
        <RadioButton x:Name="option1920" Content="1920X1080" GroupName="ResolutionM" HorizontalAlignment="Left" Height="20" Margin="11,87,0,0" VerticalAlignment="Top" Width="88" IsChecked="True"/>
        <RadioButton x:Name="option1280" Content="1280X720" GroupName="ResolutionM" HorizontalAlignment="Left" Height="20" Margin="11,107,0,0" VerticalAlignment="Top" Width="88" RenderTransformOrigin="0.49,1.995"/>





    </Grid>
</Window>
